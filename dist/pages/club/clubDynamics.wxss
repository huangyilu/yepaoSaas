/* pages/club/clubDynamics.wxss */

.clubdy-cell {
    padding: 0 30rpx;
    background-color: #22233A;
    color: #BAB9C7;
    font-size: 26rpx;
    margin-top: 20rpx;
}
.clubdy-cell .titleImg {
    width: 200rpx;
    height: 120rpx;
    float: left;
    margin: 20rpx 20rpx 20rpx 0;
}
.clubdy-cell .title-text-view {
    margin-top: 20rpx;
    width: 440rpx;
}
.clubdy-cell .title-text {
    font-size: 30rpx;
    color: white;
}
.clubdy-cell .down-view {
    padding: 20rpx 0;
    border-top: 1rpx #262439 solid;
}
.clubdy-cell .good-icon {
    width: 29rpx;
    height: 31rpx;
    float: left;
    margin-right: 10rpx;
}
.clubdy-cell .good-view {
    display: inline-block;
    margin-left: 30rpx;
}
.clubdy-cell .author-view {
    text-align: right;
}
.clubdy-cell .content-text{
    overflow:hidden;
    display:-webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2; 
}