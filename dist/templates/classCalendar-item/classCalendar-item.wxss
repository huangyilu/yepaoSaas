
.classCalendar-cell{
    height: 240rpx;
    background-color: #2f2d45;
    padding: 0 30rpx;
    margin-top: 20rpx;
}
.classCalendar-cell .title-img {
    width: 150rpx;
    height: 150rpx;
}
.classCalendar-cell .title-view {
    margin-right: 30rpx;
}
.classCalendar-cell .title-view .time-text {
    padding: 13rpx 0;
}
.classCalendar-cell .up-text-view{
    border-bottom: 1rpx #262439 solid;
    font-size: 34rpx;
    padding: 10rpx 0;
}
.up-text-view .right-text {
    margin-top:9rpx;
    float:right;
    color:#BAB9C7;
    font-size: 26rpx;
}

.classCalendar-cell .name-view .head-img {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    float: left; 
    margin-right: 20rpx;
    margin-top: 10rpx;
}
.name-headview {
    padding: 20rpx 0;
    color: #BAB9C7;
}

.appt-btn {
    width: 100rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: #FCF428;
    border-radius: 6rpx;
    border: 1rpx #FCF428 solid;
    margin-top: 57rpx;
    text-align: center;
}
.star-view .star-icon {
    width: 20rpx;
    height: 18rpx;
    margin-right: 5rpx;
}
.star-view {
    margin-left: 0;
    float: left;
    margin-top: -9rpx;
}
.otherinfo-view {
    color: #BAB9C7;
    font-size: 26rpx;
    margin-top: -10rpx;
}
.otherinfo-view .cell-icon{
    width: 30rpx;
    height: 25rpx;
    margin-right: 10rpx;
}
.otherinfo-view .text {
    margin-right: 30rpx;
}