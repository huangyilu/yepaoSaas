
.article-cell {
    padding: 30rpx;
    background-color: #2F2D45;
    margin-top: 2rpx;
}

.article-cell .left-icon {
    width: 80rpx;
    height: 80rpx;
    float: left;
    margin-top: 5rpx;
}

.article-cell .right-view {
    margin-left: 110rpx
}
.article-cell .right-view .up-view .time-text {
    float: right;
    color: #848296;
    font-size: 26rpx;
    margin-top: 7rpx;
}
.article-cell .right-view .down-view {
    overflow: hidden;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    color: #BAB9C7;
    font-size: 28rpx;
}